<% layout("/layouts/boilerplate") %>
<div class="row mt-3">
    <h1 class="col-6 offset-3">login for WanderLust</h1>
<div class="col-6 offset-3">
<form action="/login" method="POST" class="needs-validation" novalidate>
    <div class="mb-3">
    <lable for="username" class="form-label">username</lable>
    <input type="text" name="username" class="form-control" required id="username">
        
</div>

<div class="mb-3">
    <lable for="password" class="form-label">password</lable>
    <input type="password" name="password" class="form-control" required id="password">
       
</div>
<button class="btn-success">login</button>
</form>

</div>
</div>